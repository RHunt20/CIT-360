import java.util.ArrayList;

public class arrayList{
	
	public static void main(String[] args){
		ArrayList myFriendsList = new ArrayList();
		
		// Add vaules to Array //
		myFriendsList.add("Allison");
		myFriendsList.add("Shane");
		myFriendsList.add("Ben");
		myFriendsList.add("David");
		myFriendsList.add("Austin");
		
		
                System.out.println(myFriendsList);
		
	}
}
